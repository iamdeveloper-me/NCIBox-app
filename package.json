{
  "name": "NCIBox",
  "version": "1.0.0",
  "productName": "NCIBox",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "electron-build": "ng build --prod && electron .",
    "windows-build-ia32": "ng build --prod && electron-packager . NCIBox --overwrite --platform=win32 --arch=ia32 --prune=true --out=release-builds",
    "windows-build-64": "ng build --prod && electron-packager . NCIBox --overwrite --platform=win32 --prune=true --out=release-builds",
    "mac-build": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "linux-build": "ng build --prod && electron-packager . --overwrite --platform=linux --arch=x64 --prune=true --out=release-builds"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.3",
    "@angular/common": "^5.2.3",
    "@angular/compiler": "^5.2.3",
    "@angular/core": "^5.2.3",
    "@angular/forms": "^5.2.3",
    "@angular/http": "^5.2.3",
    "@angular/platform-browser": "^5.2.3",
    "@angular/platform-browser-dynamic": "^5.2.3",
    "@angular/router": "^5.2.3",
    "angular-2-daterangepicker": "^1.1.44",
    "angular-countries": "^1.1.5",
    "angular-webstorage-service": "^1.0.2",
    "angular2-signaturepad": "^2.8.0",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "electron-single-instance": "0.0.2",
    "jquery": "^3.3.1",
    "jw-bootstrap-switch-ng2": "^1.0.10",
    "moment": "^2.22.2",
    "mydatepicker": "^2.6.4",
    "ng2-datepicker": "^2.2.4",
    "ng4-loading-spinner": "^1.1.1",
    "rxjs": "^5.5.6",
    "systemjs": "^0.20.19",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.6.6",
    "@angular/compiler-cli": "^5.2.3",
    "@angular/language-service": "^5.2.3",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/jquery": "^3.3.0",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "electron": "^1.8.7",
    "electron-packager": "^10.1.2",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.3.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
